{
  "compilerOptions": {
    "target": "ES2021",
    "module": "system",
    "moduleResolution": "node",
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "strict": true,
    "noEmitOnError": false,
    "declaration": true,
    "noUncheckedIndexedAccess": true,
    "jsx": "react-jsx"
  },
  "exclude": [
    "node_modules",
    ".npm"
  ]
}
